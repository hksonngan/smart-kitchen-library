all:
	cd src;$(MAKE)
	cp src/libskl_<%= module_downcase_name %>.a ../../lib
#	cd Windows; ./generate_vcproj.rb > <%= module_name %>Base.vcproj

.PHONY: clean debug

check-syntax:
	g++ -o null -Wall -S ${CHK_SOURCES}

debug:
	cd src;$(MAKE) debug
	cp src/libskl_<%= module_downcase_name %>_d.a ../../lib

clean:
	cd src;$(MAKE) clean
