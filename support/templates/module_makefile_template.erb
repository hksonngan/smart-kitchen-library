all:
	cd src;$(MAKE)
	cp src/libskl_<%= module_downcase_name %>.a ../../lib
<%if visual_studio %>	cd VisualStudioProject; ./generate_vcproj.rb > SKL<%= module_name %>.vcproj<%else%><%end%>

.PHONY: clean debug

check-syntax:
	g++ -o null -Wall -S ${CHK_SOURCES}

debug:
	cd src;$(MAKE) debug
	cp src/libskl_<%= module_downcase_name %>_d.a ../../lib

clean:
	cd src;$(MAKE) clean
